{
    "[python]": {
        "editor.formatOnType": true,
        "editor.defaultFormatter": "ms-python.python",
    },
    "editor.inlineSuggest.enabled": true,
    "workbench.iconTheme": "icons",
    "workbench.colorTheme": "One Dark Pro Mix",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "variable.parameter, variable.language, variable.parameter.function.language.python",
                "settings": {
                    "foreground": "#ABB2BF",
                    "fontStyle": ""
                }
            },
            {
                "scope": [
                    "keyword.control.flow.python",
                    "keyword.control.import.python",
                    "keyword.operator.logical.python"
                ],
                "settings": {
                    "fontStyle": ""
                }
            },
            {
                "scope": "constant, storage.type.function, storage.type.class",
                "settings": {
                    "foreground": "#C678DD",
                }
            },
            {
                "scope": [
                    "variable.other.readwrite",
                    "entity.name.namespace",
                    "constant.other.caps.python",
                ],
                "settings": {
                    "foreground": "#ABB2BF",
                    "fontStyle": ""
                }
            },
            {
                "scope": [
                    "entity.name.type.class",
                    "meta.function-call.generic",
                    "punctuation.separator.colon.python",
                    "keyword.operator.assignment.python",
                    "keyword.operator.comparison",
                    "keyword.operator.arithmetic",
                    "keyword.operator.unpacking.arguments.python"
                ],
                "settings": {
                    "foreground": "#61afef"
                }
            },
            {
                "scope": "variable.parameter.function-call",
                "settings": {
                    "foreground": "#aa4926"
                }
            },
            {
                "scope": "support.function.builtin, support.type.python, support.type",
                "settings": {
                    "foreground": "#8888c6"
                }
            },
            {
                "scope": "comment.line.number-sign.python, punctuation.definition.comment",
                "settings": {
                    "foreground": "#5c6370"
                }
            },
            {
                "scope": "storage",
                "settings": {
                    "foreground": "#98C379"
                }
            },
            {
                "scope": "variable.language.special.self.python, variable.parameter.function.language.special.self.python",
                "settings": {
                    "foreground": "#94558d",
                }
            },
            {
                "scope": "support.function.magic.python",
                "settings": {
                    "foreground": "#b200b2",
                }
            }
        ]
    },
    "workbench.colorCustomizations": {
        "editorBracketHighlight.foreground1": "#e8ba36",
        "editorBracketHighlight.foreground2": "#48a756",
        "editorBracketHighlight.foreground3": "#309ef3",
        "editorBracketHighlight.foreground4": "#445fba",
        "editorBracketHighlight.foreground5": "#1a9286",
    },
    "editor.lineHeight": 1.6,
    "editor.fontFamily": "'Jetbrains Mono'",
    "editor.fontSize": 13.5,
    "editor.fontLigatures": true,
    "editor.semanticTokenColorCustomizations": {
        "enabled": false,
    },
    "git.enableSmartCommit": true,
    // change black formatter max line size to 119
    "python.formatting.blackArgs": [
        "--line-length",
        "140",
    ],
    "python.analysis.diagnosticSeverityOverrides": {
        "reportGeneralTypeIssues": "warning",
    },
    "python.linting.pycodestyleCategorySeverity.W": "Hint",
    "python.linting.pycodestyleEnabled": true,
    "python.linting.pycodestyleArgs": [
        "--max-line-length=160",
        "--ignore=E203"
    ],
    "python.linting.pycodestyleCategorySeverity.E": "Hint",
    "files.autoSave": "afterDelay",
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": true,
        "scminput": false,
        "yaml": false
    },
    "python.formatting.provider": "black",
    "python.terminal.activateEnvironment": false,
    "indentOnPaste.endingBlocks": [
        "}",
        "]",
        ">",
        "</",
        "<?end",
        "<?phpend",
        "<?else",
        "<?phpelse"
    ],
    "update.enableWindowsBackgroundUpdates": false,
    "update.mode": "manual",
    "editor.minimap.enabled": false,
